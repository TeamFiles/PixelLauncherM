<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.nexuslauncher.allapps.SearchResultSmallIconRow
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:minHeight="@dimen/search_result_small_row_height"
    android:paddingStart="8.0dip"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:launcher="http://schemas.android.com/apk/res-auto">
    <com.google.android.apps.nexuslauncher.allapps.SearchResultIcon
        android:ellipsize="end"
        android:id="@id/icon"
        android:layout_width="@dimen/search_row_icon_size"
        android:layout_height="@dimen/search_row_icon_size"
        android:layout_margin="@dimen/search_result_small_vertical_margin"
        android:drawableTint="?android:textColorPrimary"
        launcher:iconDisplay="search_result_small"
        launcher:matchTextInsetWithQuery="true"
        style="@style/BaseIcon" />
    <LinearLayout
        android:layout_gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@id/text_rows"
        android:paddingTop="@dimen/search_result_hero_subtitle_size"
        android:paddingBottom="@dimen/search_result_hero_subtitle_size"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:layout_weight="1.0"
        android:paddingStart="@dimen/dynamic_grid_edge_margin"
        android:paddingEnd="@dimen/dynamic_grid_edge_margin"
        style="@style/AllAppsSearchResult">
        <TextView
            android:textSize="@dimen/search_result_hero_title_size"
            android:textColor="?android:textColorPrimary"
            android:ellipsize="end"
            android:gravity="start|center"
            android:id="@id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:singleLine="true"
            android:textAlignment="viewStart"
            android:paddingEnd="@dimen/search_result_small_horizontal_padding"
            style="@style/AllAppsSearchResult" />
        <TextView
            android:textSize="@dimen/search_result_hero_subtitle_size"
            android:textColor="?android:textColorPrimary"
            android:gravity="start|center"
            android:id="@id/delimeter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="â€¢"
            android:maxLines="1" />
        <TextView
            android:textSize="@dimen/search_result_hero_subtitle_size"
            android:textColor="?android:textColorSecondary"
            android:ellipsize="end"
            android:id="@id/subtitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:singleLine="true"
            android:paddingStart="@dimen/search_result_small_horizontal_padding"
            style="@style/AllAppsSearchResult" />
    </LinearLayout>
    <com.google.android.apps.nexuslauncher.allapps.SearchResultIcon
        android:textSize="@dimen/search_hero_inline_button_text_size"
        android:layout_gravity="center"
        android:id="@id/shortcut_0"
        android:layout_width="@dimen/search_row_icon_size"
        android:layout_height="fill_parent"
        android:layout_marginEnd="@dimen/search_result_margin"
        launcher:iconDisplay="search_result_small"
        launcher:layoutHorizontal="true"
        style="@style/BaseIcon" />
    <com.google.android.apps.nexuslauncher.allapps.SearchResultIcon
        android:textSize="@dimen/search_hero_inline_button_text_size"
        android:layout_gravity="center_vertical"
        android:id="@id/shortcut_1"
        android:layout_width="@dimen/search_row_icon_size"
        android:layout_height="fill_parent"
        android:layout_marginEnd="@dimen/search_result_margin"
        launcher:iconDisplay="search_result_small"
        launcher:layoutHorizontal="true"
        style="@style/BaseIcon" />
    <com.google.android.apps.nexuslauncher.allapps.SearchResultIcon
        android:textSize="@dimen/search_hero_inline_button_text_size"
        android:layout_gravity="center_vertical"
        android:id="@id/shortcut_2"
        android:layout_width="@dimen/search_row_icon_size"
        android:layout_height="fill_parent"
        android:layout_marginEnd="@dimen/search_result_margin"
        launcher:iconDisplay="search_result_small"
        launcher:layoutHorizontal="true"
        style="@style/BaseIcon" />
    <FrameLayout
        android:layout_gravity="center"
        android:id="@id/settings_search_button_container"
        android:visibility="gone"
        android:layout_width="@dimen/search_row_icon_size"
        android:layout_height="@dimen/search_row_icon_size"
        android:layout_marginHorizontal="@dimen/search_result_margin"
        android:layout_marginVertical="@dimen/search_result_small_vertical_margin">
        <ImageButton
            android:layout_gravity="center"
            android:id="@id/settings_search_button"
            android:background="@drawable/circle_accentprimary_32dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_search" />
    </FrameLayout>
    <FrameLayout
        android:layout_gravity="center_vertical"
        android:id="@id/suggest_entity_container"
        android:visibility="gone"
        android:layout_width="@dimen/search_row_icon_size"
        android:layout_height="@dimen/search_row_icon_size"
        android:layout_marginStart="@dimen/suggest_image_vertical_padding"
        android:layout_marginVertical="@dimen/suggest_image_vertical_padding">
        <ImageView
            android:layout_gravity="center"
            android:id="@id/suggest_entity"
            android:background="@drawable/entity_image_background"
            android:layout_width="@dimen/entity_image_size"
            android:layout_height="@dimen/entity_image_size"
            android:adjustViewBounds="true"
            android:clipToOutline="true" />
    </FrameLayout>
    <ImageButton
        android:layout_gravity="center"
        android:id="@id/query_builder"
        android:background="@android:color/transparent"
        android:visibility="gone"
        android:layout_width="@dimen/query_builder_size"
        android:layout_height="@dimen/query_builder_size"
        android:src="@drawable/ic_query_builder"
        android:layout_marginStart="@dimen/query_builder_padding_start"
        android:layout_marginEnd="@dimen/query_builder_padding_end" />
</com.google.android.apps.nexuslauncher.allapps.SearchResultSmallIconRow>